{
  "name": "map-play",
  "version": "1.2.0",
  "description": "Map adventures",
  "private": true,
  "author": "Liz Shigetoshi",
  "repository": "https://github.com/lshig/map-play.git",
  "license": "MIT",
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "build": "NODE_ENV=production webpack --config config/webpack.prod.js",
    "deploy": "gh-pages -d dist",
    "eslint": "eslint '**/*.js'",
    "format": "prettier --write '**/*.(js|scss|css|md)'",
    "mdlint": "markdownlint-cli2 'README.md'",
    "lint": "npm run format && npm run eslint && npm run stylelint && npm run mdlint",
    "serve": "NODE_ENV=production webpack serve --config config/webpack.prod.js",
    "start": "NODE_ENV=development webpack serve --config config/webpack.dev.js",
    "stylelint": "stylelint '**/*.scss'",
    "test": "npm run lint && jest --coverage",
    "test:watch": "npm test -- --watch"
  },
  "dependencies": {
    "mapbox-gl": "^1.9.1",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.13",
    "@babel/plugin-proposal-class-properties": "^7.12.13",
    "@babel/preset-env": "^7.12.13",
    "@babel/preset-react": "^7.12.13",
    "@babel/runtime": "^7.12.13",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^5.0.2",
    "css-minimizer-webpack-plugin": "^1.2.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "enzyme-to-json": "^3.6.1",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-react": "^7.22.0",
    "favicons": "^6.2.0",
    "favicons-webpack-plugin": "^5.0.1",
    "gh-pages": "^3.1.0",
    "html-webpack-plugin": "^5.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "markdownlint-cli2": "0.0.13",
    "mini-css-extract-plugin": "^1.3.6",
    "postcss-loader": "^5.0.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.2.1",
    "regenerator-runtime": "^0.13.7",
    "sass": "^1.32.7",
    "sass-loader": "^11.0.1",
    "style-loader": "^2.0.0",
    "stylelint": "^13.9.0",
    "stylelint-config-standard": "^20.0.0",
    "webpack": "^5.21.2",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.7.3"
  }
}
